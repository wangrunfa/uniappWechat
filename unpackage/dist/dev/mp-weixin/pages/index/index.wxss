  height: 410rpx;
  width: 710rpx;
  margin: 0 auto;
}
.nav-maxBox-left{
width: 355rpx;
height: 400rpx;
float: left;
}
.shadow-blur {
	position: relative;
}
.nav-maxBox-right{
width: 355rpx;
height: 400rpx;
float: left;
}
.nav-maxBox-left-button{
  width: 345rpx;
  height: 370rpx;
  margin: 20rpx 0;
  border-radius: 7px;
 background:white;
 overflow: hidden;
}
.nav-maxBox-left-button-image{
	width: 345rpx;
	height: 370rpx;
}
.nav-maxBox-right-button-01{
 width: 345rpx;
  height: 175rpx;
  margin: 20rpx 0 0 10rpx;
  border-radius: 7px;
 background:rgba(255, 255, 255);
 overflow: hidden
}
.nav-maxBox-right-button-01>image{
   width: 345rpx;
