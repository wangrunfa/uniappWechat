
@font-face {
	        font-family: 'iconfont';
	        src: url('https://at.alicdn.com/t/font_865816_17gjspmmrkti.ttf') format('truetype');
}
.test {
	        font-family: iconfont;
	        margin-left: 20rpx;
}
.swiperMaxBox{
			position: relative;
}
.swiperImage{
			width: 710rpx;
			margin: 0 20rpx;
			border-radius: 20rpx;
			height: 710rpx;
			position: absolute;
			z-index: 60;
}
.swiperTitle{
				position: absolute;
				width: 650rpx;
				height: 170rpx;
			background: rgba(0,0,0,0.2);
				top: 500rpx;
				left: 50rpx;
				border-radius: 30rpx;
				z-index: 120;
}
.swiperMaxTitle,.swiperMinTitle{
			color: #ffffff;
			position: absolute;
			font-weight: bold;
}
.swiperMaxTitle{
			
			right: 15rpx;
			top:15rpx;
			font-size: 60rpx;
}
.swiperMinTitle{
		
			top: 100rpx;
			left: 30px;
			font-size: 35rpx;
}
.popups-style-box{
			height: 500rpx;
			width: 710rpx;
			margin: 0 20rpx;
			border-top-left-radius:30rpx ;
			border-top-right-radius:30rpx ;
			
			background: #fff;
}
.slideshow-box{
		width: 750rpx;
		height: 710rpx;
}
