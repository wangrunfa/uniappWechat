}
.image-group-box {
  width: 730rpx;
  margin: 10rpx auto 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  white-space: nowrap;
}
.titles{
  height: 30px;
  text-indent: 30rpx;
  font-size: 15px;

  font-weight: bold;
  border-left: 3px solid blue;
  line-height: 30px;
}
.email-titles{
  height: 50px;
  width: 710rpx;
  margin: 20rpx;
  color: rgb(238, 186, 42);
  text-indent: 30rpx;
  font-size: 17px;
  line-height: 50px;
  background: rgb(255, 255, 255);
  text-align: center;
  border-radius: 7px;
  font-weight: bold;
  size: 15px;
}


